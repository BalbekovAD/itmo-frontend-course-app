{
    "name": "itmo-frontend-course-app",
    "private": true,
    "version": "0.0.1",
    "type": "module",
    "scripts": {
        "dev": "vite dev",
        "build": "vite build",
        "preview": "vite preview",
        "prepare": "svelte-kit sync || echo ''",
        "check-supressed": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --compiler-warnings css_unused_selector:ignore,a11y_no_static_element_interactions:ignore,a11y_click_events_have_key_events:ignore",
        "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --compiler-warnings css_unused_selector:ignore",
        "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
        "format": "prettier --write .",
        "lint": "prettier --check . && eslint ."
    },
    "devDependencies": {
        "@eslint/compat": "^1.2.5",
        "@eslint/js": "^9.18.0",
        "@sveltejs/adapter-netlify": "^4.4.0",
        "@sveltejs/enhanced-img": "^0.4.4",
        "@sveltejs/kit": "^2.16.0",
        "@sveltejs/vite-plugin-svelte": "^5.0.0",
        "eslint": "^9.18.0",
        "eslint-config-prettier": "^10.0.1",
        "eslint-plugin-svelte": "^3.0.0",
        "flowbite": "^3.1.2",
        "flowbite-svelte": "^0.48.6",
        "globals": "^16.0.0",
        "prettier": "^3.4.2",
        "prettier-plugin-svelte": "^3.3.3",
        "svelte": "^5.0.0",
        "svelte-check": "^4.2.1",
        "typescript": "^5.0.0",
        "typescript-eslint": "^8.20.0",
        "vite": "^6.0.0"
    },
    "dependencies": {
        "@lottiefiles/dotlottie-web": "^0.44.0",
        "@supabase/supabase-js": "^2.49.4",
        "@tailwindcss/vite": "^4.1.4",
        "flowbite-svelte-icons": "^2.1.1",
        "lottie-web": "^5.12.2",
        "marked": "^15.0.12",
        "pdfjs-dist": "5.3.31",
        "strapi-sdk-js": "^3.0.0",
        "tailwindcss": "^4.1.4"
    }
}
